				<code profiles="default">set SkillDefinition'GD_Assassin_Skills.Cunning.Sanic' SkillEffectPresentations AttributePresentationDefinition'GD_Assassin_Skills.Cunning.Sanic:APD'</code>
				<hotfix name="Hotfix" package="GD_Assassin_Streaming">
					<code profiles="default">set SkillTreeBranchDefinition'GD_Assassin_Skills.SkillTree.Branch_Cunning' Tiers[0].Skills (SkillDefinition'GD_Assassin_Skills.Cunning.FastHands',SkillDefinition'GD_Assassin_Skills.Cunning.Sanic',SkillDefinition'GD_Assassin_Skills.Cunning.CounterStrike')</code>
